package cl.udla.ia.domain;

import java.util.List;

public class ArbolBinario implements EstructuraDatos<Nodo>{

	private Nodo raiz;
	
	public ArbolBinario(){

	}
	
	@Override
	public void insert(Nodo value) {
		Nodo nodoEncontrado = find(value);
		if (null == nodoEncontrado) {
			if (null == raiz) {
				raiz = new Nodo();
				raiz.setNumero(value.getNumero());
				System.out.println("Numero " + raiz.getNumero() + " agregado al arbol binario");
			}
			
		}else {
			System.out.println("Nodo ya existe en arbol");
		}
		
		// TODO Auto-generated method stub
		
	}

	@Override
	public void remove(Nodo value) {
		// TODO Auto-generated method stub
		
	}

	@Override
	public Nodo find(Nodo key) {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public List<Nodo> get() {
		// TODO Auto-generated method stub
		return null;
	}

	public Nodo getRaiz() {
		return raiz;
	}

	public void setRaiz(Nodo raiz) {
		this.raiz = raiz;
	}


}
